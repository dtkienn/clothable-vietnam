              <path fill-rule="evenodd" clip-rule="evenodd" d="M42.4417 15.7777V2.54871H37.8987V0.199707H49.5337V2.54871H44.9907V15.7777H42.4417Z" fill="#FFFFFF"/>
              <path fill-rule="evenodd" clip-rule="evenodd" d="M62.197 15.977C58.394 15.977 56.031 13.714 56.031 10.07V5.906C56.031 2.263 58.394 0 62.197 0C66 0 68.363 2.248 68.363 5.867V10.07C68.363 13.714 66 15.977 62.197 15.977ZM62.197 2.33C59.797 2.33 58.581 3.527 58.581 5.886V10.07C58.581 12.457 59.797 13.667 62.197 13.667C64.597 13.667 65.813 12.457 65.813 10.07V5.906C65.813 3.5 64.63 2.33 62.197 2.33Z" fill="#FFFFFF"/>
